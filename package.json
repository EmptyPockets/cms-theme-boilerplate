{
  "name": "@hubspot/cms-theme-boilerplate",
  "version": "1.3.0",
  "description": "Boilerplate project for building websites on the HubSpot CMS",
  "repository": {
    "type": "git",
    "url": "git@git.hubteam.com:HubSpot/cms-theme-boilerplate.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.10.4",
    "@hubspot/cms-cli": "^1.1.4",
    "@hubspot/webpack-cms-plugins": "^1.1.7",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.6.5",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-plugin-import": "^2.18.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fast-sass-loader": "^1.5.0",
    "import-glob-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.12.0",
    "node-sass-glob-importer": "^5.3.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.0",
    "sass-resources-loader": "^2.0.3",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "start": "webpack --watch --env.autoupload --mode production",
    "also": "webpack --watch --env.autoupload --mode production",
    "build": "rm -Rf dist && webpack --mode production",
    "deploy": "rm -Rf dist && webpack --mode production && hs upload dist damore-cms-theme",
    "lint": "eslint src && prettier --list-different 'src/**/*.js' 'src/**/*.json'",
    "prettier:write": "prettier --write 'src/**/*.js' 'src/**/*.json'",
    "scss": "node-sass -w src/scss -o src/css",
    "dev": "concurrently \"npm run scss\" \"npm run start\""
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "last 2 versions",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "flickity-fade": "^1.0.0"
  }
}
